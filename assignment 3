import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a positive integer: ");
        int num = sc.nextInt();
        
        for (int i = 1; i <= num; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j);
                
                // Print a space except for the very last number on the last line
                if (!(i == num && j == num)) {
                    System.out.print(" ");
                }
            }
            System.out.println();
        }
    }
}
